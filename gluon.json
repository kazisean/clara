{
  "name": "Clara",
  "vendor": "hossain.cc",
  "appId": "cc.hossain.browser",
  "binaryName": "Clara-browser",
  "version": {
    "product": "firefox",
    "version": "121.0"
  },
  "buildOptions": {
    "generateBranding": true
  },
  "addons": {
    "ublock": {
      "platform": "github",
      "id": "uBlock0@raymondhill.net",
      "repo": "gorhill/uBlock",
      "version": "1.54.0",
      "fileGlob": "uBlock0_*.firefox(.signed)?.xpi"
    }
  },
  "brands": {
    "stable": {
      "backgroundColor": "#2B2A33",
      "brandShorterName": "Clara",
      "brandShortName": "Clara Browser",
      "brandFullName": "Clara Browser",
      "release": {
        "displayVersion": "0.0.1",
        "github": {
          "repo": "kazisean/clara"
        },
        "x86": {
          "windowsMar": "windows.mar",
          "macosMar": "macosIntel.mar",
          "linuxMar": "linux.mar"
        }
      }
    }
  },
  "license": {
    "ignoredFiles": [
      ".*\\.json"
    ],
    "licenseType": "MPL-2.0"
  }
}